          {
            
            "format": "table",
            "rawQuery": true,
            "rawSql": "SELECT sum(EXTRACT(EPOCH FROM delta_T)) as total_on_time\r\nfrom(\r\nSELECT\r\n  on_segment,\r\n  MAX(time) - MIN(time) AS delta_T\r\nFROM\r\n  (\r\n    SELECT\r\n      time,\r\n      SUM(on_id) OVER (\r\n        ORDER BY\r\n          time\r\n      ) AS on_segment\r\n    FROM\r\n      (\r\n        SELECT\r\n          time,\r\n          CASE\r\n            WHEN machine_status = 'on'\r\n            AND LAG(machine_status) OVER (\r\n              ORDER BY\r\n                time\r\n            ) != 'on' THEN 1\r\n            ELSE 0\r\n          END AS on_id\r\n        FROM\r\n          dmg_data\r\n        where\r\n          machine_id = '$machine_id'\r\n          and $__timeFilter(time)\r\n      )\r\n  )\r\nGROUP BY\r\n  on_segment\r\n  );\r\n-- SELECT \r\n--   time,\r\n--   on_segment,\r\n--   off_segment,\r\n--   machine_status\r\n--   -- MAX(time) - MIN(time) AS delta_T\r\n-- FROM(\r\n--   SELECT\r\n--     time,\r\n--     machine_status,\r\n--     SUM(off_id) OVER (\r\n--       ORDER BY\r\n--         time\r\n--     ) AS off_segment,\r\n--     SUM(on_id) OVER (\r\n--       ORDER BY\r\n--         time\r\n--     ) AS on_segment\r\n--   FROM\r\n--     (\r\n--       SELECT\r\n--         time,\r\n--         machine_status,\r\n--         CASE\r\n--           WHEN machine_status = 'off'\r\n--           AND LAG(machine_status) OVER (\r\n--             ORDER BY\r\n--               time\r\n--           ) != 'off' THEN 1\r\n--           ELSE 0\r\n--         END AS off_id,\r\n--         CASE\r\n--           WHEN machine_status = 'on'\r\n--           AND LAG(machine_status) OVER (\r\n--             ORDER BY\r\n--               time\r\n--           ) != 'on' THEN 1\r\n--           ELSE 0\r\n--         END AS on_id\r\n--       FROM\r\n--         dmg_data\r\n--       where machine_id = '$machine_id' and $__timeFilter(time)\r\n--     )\r\n--   )\r\n-- -- where on_segment > 0 and off_segment > 0\r\n-- ;",
            "refId": "A",
